        </activity>
        <activity
            android:name="com.example.simplemedicalexam.InputPage4"
            android:label="@string/title_activity_input_page4"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.example.simplemedicalexam.OutputPage"
            android:label="@string/title_activity_output_page"
